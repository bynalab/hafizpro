{
  "1": {
    "t": "(Muhammad), say, \"He is the only God.",
    "tr": "Qul huwal laahu ahad"
  },
  "2": {
    "t": "God is Absolute.",
    "tr": "Allah-hus samad"
  },
  "3": {
    "t": "He neither begets nor was He begotten.",
    "tr": "Lam yalid Wa-lam yoolad"
  },
  "4": {
    "t": "There is no one equal to Him.",
    "tr": "Wa-lam yakul lahoo kufuwan ahad"
  }
}